{
  "testMatch": [
    "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
    "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
  ],
  "setupFiles": ["src/**/__tests__/**.js"],
  "transform": { ".*": "/node_modules/babel-jest" },
  "moduleFileExtensions": ["js"],
  "collectCoverageFrom": ["src/*.js", "!src/index.js"]
}
